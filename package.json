    "cheerio": "^1.0.0-rc.12",
    "commander": "^11.1.0",
    "delay": "^5.0.0",
    "dotenv": "^16.3.1",
    "faker": "^5.5.3",
    "fs-extra": "^11.2.0",
    "https-proxy-agent": "^7.0.2",
    "proxy-agent": "^6.3.1",
    "proxy-chain": "^2.4.0",
    "puppeteer": "^24.20.0",
