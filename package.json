    "test:google": "node tests/test-google.js",
    "test:kilocode": "node tests/test-kilocode.js",
    "test:int": "node tests/test-integration.js",
    "test": "npm run test:env && npm run test:proxy && npm run test:google && npm run test:kilocode && npm run test:int",
    "dev": "node --inspect src/main.js",
